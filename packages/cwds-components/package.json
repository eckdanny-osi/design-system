{
  "name": "@cwds/components",
  "version": "0.2.0-alpha.13",
  "description": "React components for CWDS",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "style": "lib/index.css",
  "author": "Danny Eck <danny.eck@osi.ca.gov>",
  "license": "AGPL-3.0",
  "private": false,
  "scripts": {
    "//prepare": "yarn run build",
    "//prepublishOnly": "TODO: clean up the package.json file",
    "build": "yarn build:make",
    "build:make": "run-p build:make:*",
    "build:make:css": "gulp build:make:css",
    "build:make:js:es": "NODE_ENV=production babel ./src --out-dir ./lib --ignore *.test.js,*.stories.js",
    "build:make:js:esm": "NODE_ENV=production BABEL_ENV=modules babel ./src/index.js --out-file ./lib/index.esm.js",
    "build:make:js": "run-p build:make:js:*",
    "build:make:md": "",
    "build:make:tsd": "gulp build:make:tsd",
    "build:watch": "run-p build:watch:*",
    "build:watch:css": "gulp build:watch:css",
    "build:watch:js:es": "yarn build:make:js:es --watch",
    "build:watch:js:esm": "yarn build:make:js:esm --watch",
    "build:watch:js": "run-p build:watch:js:*",
    "build:watch:md": "echo 1",
    "build:watch:tsd": "gulp build:watch:tsd",
    "clean": "mkdir -p lib && rimraf lib*",
    "start": "yarn storybook -p 3002",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "NODE_ENV=development jest --env=jsdom",
    "serve:coverage": "serve coverage",
    "test:watch:coverage": "run-p \"test --watch --coverage\" serve:coverage"
  },
  "dependencies": {
    "@cwds/validation": "^0.0.1",
