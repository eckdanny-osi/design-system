{
  "name": "@cwds/datagrid",
  "version": "0.4.0",
  "description": "Datagrids for CWS-CARES",
  "keywords": [
    "react-table",
    "datagrid"
  ],
  "author": "Danny Eck <danny.eck@osi.ca.gov>",
  "homepage": "https://cws-cares.netlify.com/apps/datagrid",
  "license": "AGPL-3.0",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf dist",
    "build": "run-p build:js build:css",
    "build:es": "babel src --out-dir dist --ignore \"src/**/*.stories.js\",\"src/**/*.test.js\",\"__tests__\"",
    "build:js": "rollup -c rollup.config.js",
    "build:css": "node-sass -r -o dist --source-map true --importer ../../sass-importers.js src",
    "prebuild:watch": "rimraf dist",
    "build:watch": "run-p build:watch:*",
    "build:watch:es": "yarn build:es -w",
    "//build:watch:css": "run-p build:watch:css:*",
    "//build:watch:css:deps": "onchange '../core/scss/**/*.scss' -- yarn build:css",
    "//build:watch:css:self": "yarn build:css && yarn build:css -w"
  },
  "dependencies": {
    "@cwds/core": "^0.4.0",
    "@cwds/icons": "^0.4.0",
    "@cwds/reactstrap": "^0.4.0",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "lodash.omit": "^4.5.0",
    "lodash.uniqueid": "^4.0.1",
    "react-select": "^1.3",
    "react-table": "^6.8.6"
  },
  "devDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4"
  },
  "peerDependencies": {
    "react": "^16.4",
    "react-dom": "^16.4"
  }
}
