{
  "name": "@cwds/forms",
  "version": "1.1.1",
  "description": "Forms for CWS-CARES",
  "author": "Danny Eck <danny.eck@osi.ca.gov>",
  "homepage": "https://cws-cares.netlify.com",
  "license": "AGPL-3.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "scss",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "prebuild": "yarn clean",
    "build": "run-p build:*",
    "build:js": "run-s build:js:tsc build:js:babel build:js:rollup",
    "build:js:tsc": "tsc -p tsconfig.json",
    "build:js:babel": "babel compiled --verbose --root-mode upward --out-dir dist",
    "build:js:rollup": "rollup -c",
    "postbuild:js": "rimraf compiled",
    "build:scss": "cpx --verbose \"src/**/*.{css,scss}\" dist",
    "predev": "yarn clean",
    "dev": "run-p dev:*",
    "dev:esm": "babel src --verbose --root-mode upward --watch --out-dir dist --extensions \".ts,.tsx\"",
    "dev:scss": "yarn build:scss --watch"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.3.3",
    "@cwds/reactstrap": "1.1.1",
    "@types/classnames": "^2.2.7",
    "classnames": "^2.2.6",
    "lodash.uniqueid": "^4.0.1",
    "react-select": "^2.1.1"
  },
  "devDependencies": {
    "@types/react-select": "^2.0.6",
    "@types/reactstrap": "^6.4.2",
    "formik": "^1.3.1",
    "prop-types": "^15.6.2",
    "react": "^16.4"
  },
  "peerDependencies": {
    "react": "^16.4",
    "react-dom": "^16.4"
  }
}
