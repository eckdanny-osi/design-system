{
  "name": "@cwds/icons",
  "version": "1.0.0-rc.5",
  "description": "Icons for CWS CARES",
  "author": "Danny Eck <danny.eck@osi.ca.gov>",
  "homepage": "https://cws-cares.netlify.com",
  "license": "AGPL-3.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "scss",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "prebuild": "yarn clean",
    "build": "run-p build:*",
    "build:esm": "babel --verbose --root-mode upward --out-dir dist src",
    "build:scss": "cpx --verbose \"src/**/*.{css,scss}\" dist",
    "build:cjs": "rollup -c",
    "clean": "rimraf dist",
    "dev": "run-p dev:*",
    "dev:esm": "yarn build:esm --watch",
    "dev:scss": "yarn build:scss --watch"
  },
  "dependencies": {
    "@cwds/core": "^1.0.0-rc.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.5",
    "@fortawesome/free-regular-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "@fortawesome/react-fontawesome": "^0.1.3"
  },
  "devDependencies": {
    "react": "^16.4"
  },
  "peerDependencies": {
    "react": "^16.4",
    "react-dom": "^16.4"
  }
}
