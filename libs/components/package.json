{
  "name": "@cwds/components",
  "version": "0.7.0",
  "description": "> TODO: description",
  "author": "Danny Eck <danny.eck@osi.ca.gov>",
  "homepage": "https://cws-cares.netlify.com",
  "bugs": {
    "url": "https://github.com/ca-cwds/design-system/issues"
  },
  "license": "AGPL-3.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "directories": {
    "lib": "dist",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "prebuild": "rimraf dist",
    "build": "run-p build:es build:cjs build:css",
    "build:es": "babel --root-mode upward src --out-dir dist --ignore \"src/**/*.stories.js\",\"src/**/*.test.js\"",
    "build:cjs": "rollup src/index.js -c rollup.config.js --format cjs --file dist/index.cjs.js",
    "build:css": "node-sass -r -o dist --source-map true --importer ../../sass-importers.js src",
    "prebuild:watch": "rimraf dist",
    "build:watch": "run-p build:watch:*",
    "build:watch:es": "yarn build:es -w",
    "build:watch:css": "run-p build:watch:css:*",
    "build:watch:css:deps": "onchange '../core/scss/**/*.scss' -- yarn build:css",
    "build:watch:css:self": "sleep 1 && yarn build:css && yarn build:css -w"
  },
  "dependencies": {
    "@cwds/core": "^0.7.0",
    "@cwds/datagrid": "^0.7.0",
    "@cwds/icons": "^0.7.0",
    "@cwds/reactstrap": "^0.7.0",
    "bootstrap": "^4.1.3",
    "classnames": "^2.2.6",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "lodash.uniqueid": "^4.0.1",
    "react-input-mask": "^2.0.4",
    "react-select": "^2.1.1",
    "react-table": "^6.8.6"
  },
  "devDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.4"
  },
  "peerDependencies": {
    "react": "^16.4",
    "react-dom": "^16.4"
  }
}
