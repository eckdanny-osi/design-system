{
  //
  // Grays
  //
  "gray-100": "hsl(0, 0, 96)",  // ($ray)
  "gray-200": "hsl(0, 0, 91)",  // ($grouper)
  "gray-300": "hsl(0, 0, 82)",  // ($trout)
  "gray-400": "hsl(0, 0, 60)",  //
  "gray-500": "hsl(0, 0, 50)",  //
  "gray-600": "hsl(0, 0, 40)",  //
  "gray-700": "hsl(0, 0, 36)",  // ($wahoo)
  "gray-800": "hsl(0, 0, 20)",  //
  "gray-900": "hsl(0, 0, 10)",  // ($shark)
  //
  // Theme Colors
  //
  "primary":    "#09798e",      // ($aspen)
  "secondary":  "$gray-700",
  "success":    "#068347",      // ($elm)
  "info":       "#0d4f78",      // ($spruce)
  "warning":    "#e7740b",      // ($hickory)
  "danger":     "#bb3709",      // ($maple)
  "light":      "$gray-100",
  "dark":       "$gray-800",
  //
  // Spacers
  //
  "spacer": "1rem",
  "spacer-0": "$spacer * 0",
  "spacer-1": "$spacer * 0.25",
  "spacer-2": "$spacer * 0.5",
  "spacer-3": "$spacer * 1",
  "spacer-4": "$spacer * 1.5",
  "spacer-5": "$spacer * 3",
  //
  // Breakpoints
  //
  "grid-breakpoint-xs": "0",
  "grid-breakpoint-sm": "576px",
  "grid-breakpoint-md": "768px",
  "grid-breakpoint-lg": "992px",
  "grid-breakpoint-xl": "1200px",
}
