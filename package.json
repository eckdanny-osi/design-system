{
  "name": "design-system",
  "description": "CARES Design System",
  "license": "AGPL-3.0",
  "version": "latest",
  "private": true,
  "homepage": "https://cws-cares.netlify.com",
  "bugs": {
    "url": "https://github.com/ca-cwds/design-system/issues"
  },
  "scripts": {
    "test": "jest",
    "test:all": "CI=true lerna run --parallel test ",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "start": "cd apps/www && yarn react-scripts start",
    "prebuild": "lerna bootstrap",
    "build:www": "lerna run --stream --scope www build",
    "build:libs": "lerna run --scope @cwds/* build",
    "build": "yarn build:libs && yarn build:www",
    "build:watch": "lerna run --scope @cwds/* --parallel build:watch",
    "dev": "yarn build:watch",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "release": "lerna publish --conventional-commits --force-publish"
  },
  "workspaces": {
    "nohoist": [
      "**/react-scripts",
      "**/react-scripts/**",
      "www/**",
      "rails_app/**"
    ],
    "packages": [
      "libs/*",
      "apps/*"
    ]
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.4.11",
    "@storybook/addon-knobs": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lerna": "^3.1.4",
    "node-sass": "^4.9.3",
    "node-sass-js-importer": "^4.0.1",
    "node-sass-json-importer": "^4.1.0",
    "node-sass-tilde-importer": "^1.0.2",
    "node-touch": "^0.0.3",
    "npm-run-all": "^4.1.3",
    "onchange": "^5.0.2",
    "react": "^16.4",
    "react-dom": "^16.4",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "sass-loader": "^7.1.0"
  },
  "dependencies": {}
}
