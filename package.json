{
  "name": "design-system",
  "description": "CARES Design System",
  "version": "latest",
  "private": true,
  "homepage": "https://cws-cares.netlify.com",
  "bugs": {
    "url": "https://github.com/ca-cwds/design-system/issues"
  },
  "scripts": {
    "test": "jest",
    "test:all": "CI=true lerna run --parallel test ",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "start": "cd libs/www && SKIP_PREFLIGHT_CHECK=true yarn react-scripts start",
    "prebuild": "lerna bootstrap",
    "build:www": "SKIP_PREFLIGHT_CHECK=true lerna run --stream --scope www build",
    "build:libs": "lerna run --scope @cwds/* build",
    "build": "yarn build:libs && yarn build:www",
    "build:watch": "lerna run --scope @cwds/* --parallel build:watch",
    "dev": "yarn build:watch",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "workspaces": {
    "nohoist": [
      "**/react-scripts",
      "**/react-scripts/**",
      "www/**",
      "rails_app/**"
    ],
    "packages": [
      "libs/*"
    ]
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.4.11",
    "@storybook/addon-knobs": "^3.4.11",
    "@storybook/react": "^3.4.11",
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7",
    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lerna": "^3.1.4",
    "node-sass": "^4.9.3",
    "node-sass-tilde-importer": "^1.0.2",
    "npm-run-all": "^4.1.3",
    "onchange": "^5.0.2",
    "react": "^16.4",
    "react-dom": "^16.4",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "yargs": "^12.0.2"
  },
  "dependencies": {}
}
